{
  "name": "@xacro-organization/xacro-ui",
  "version": "0.0.0",
  "description": "A Vue3 UI Components for Xacro Inc.",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/xacro-ui.umd.js",
  "module": "./dist/xacro-ui.es.js",
  "author": "Xacro Inc.",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://xacro.backlog.jp/git/XACRO/xacro-ui.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:storybook": "pnpm run storybook",
    "typecheck": "tsc --noEmit",
    "build": "run-p build:*",
    "build:types": "tsc --emitDeclarationOnly",
    "build:css": "tailwindcss -i ./src/styles/style.css -o ./xacro-ui.css",
    "build:js": "vite build",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint ./src/**/*.ts{,x} src/**/*.vue",
    "lint:prettier": "prettier --check ./src/**/*.ts{,x}",
    "lint:tsc": "pnpm run typecheck",
    "format": "run-s -c format:*",
    "format:eslint": "eslint --fix ./src/**/*.ts{,x}",
    "format:prettier": "prettier --write ./src/**/*.ts{,x}",
    "test": "run-p test:*",
    "test:ci": "vitest run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate": "scaffdog generate",
    "pub": "pnpm publish --access=public ."
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.4.29"
  },
  "packageManager": "pnpm@9.2.0",
  "devDependencies": {
    "@chromatic-com/storybook": "1.6.1",
    "@rushstack/eslint-patch": "^1.8.0",
    "@storybook/addon-essentials": "^8.2.1",
    "@storybook/addon-interactions": "^8.2.1",
    "@storybook/addon-links": "^8.2.1",
    "@storybook/addon-onboarding": "^8.2.1",
    "@storybook/blocks": "^8.2.1",
    "@storybook/test": "^8.2.1",
    "@storybook/vue3": "^8.2.1",
    "@storybook/vue3-vite": "^8.2.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "esbuild": "0.23.0",
    "eslint": "^8.57.0",
    "eslint-plugin-functional": "^6.6.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prefer-arrow-functions": "^3.3.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-total-functions": "^7.1.0",
    "eslint-plugin-unused-imports": "^4.0.1",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.39",
    "prettier": "^3.2.5",
    "storybook": "^8.2.1",
    "tailwindcss": "^3.4.6",
    "typescript": "~5.1.6",
    "vite": "^5.3.3",
    "vite-plugin-dts": "^3.9.1",
    "vue-tsc": "^2.0.21"
  }
}
